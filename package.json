{
  "name": "format-poker",
  "private": true,
  "version": "0.1.0",
  "source": "src/index.html",
  "scripts": {
    "client:start": "parcel",
    "client:build": "parcel build --dist-dir dist/client",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "npm run client:build && npm run build:server",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "migrate": "psql \"$DATABASE_URL\" -f init_db.sql",
    "seed": "psql \"$DATABASE_URL\" -f seed_db.sql"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.11.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "express": "^5.1.0",
    "serve": "^14.2.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "nodemon": "^3.1.11",
    "parcel": "^2.16.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
