{
  "name": "format-poker",
  "private": true,
  "version": "0.0.0",
  "source": "src/index.html",
  "scripts": {
    "client:start": "parcel",
    "client:build": "parcel build --dist-dir dist/client",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "npm run client:build && npm run build:server",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "migrate": "psql \"$DATABASE_URL\" -f init_db.sql",
    "seed": "psql \"$DATABASE_URL\" -f seed_db.sql"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "express": "^4.18.2",
    "serve": "^14.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.13",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "nodemon": "^3.1.10",
    "parcel": "^2.14.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
